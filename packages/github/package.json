{
  "name": "@docus/github",
  "version": "1.2.6",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "prepare": "nuxt-module-build --stub && nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "lint": "eslint --ext .js,.ts,.vue . --fix",
    "release": "build && ../../.github/scripts/release.sh",
    "release:edge": "yarn build && ../../.github/scripts/release-edge.sh",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.3",
    "@octokit/graphql": "^4.8.0",
    "@octokit/rest": "^18.12.0",
    "remark-github": "^11.2.3"
  },
  "devDependencies": {
    "@nuxt/content": "npm:@nuxt/content-edge@latest",
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "^3.0.0-rc.3",
    "nuxt": "^3.0.0-rc.3",
    "vitest": "^0.14.1"
  }
}
